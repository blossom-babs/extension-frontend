{
    "manifest_version": 3,
    "name": "Email Open Tracker",
    "description": "Track email opens with tracking pixels.",
    "version": "1.0",
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "128": "/icon128.png"
      }
    }
  }
  